{
  "name": "hassu",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/api-graphql": "2.2.3",
    "@aws-amplify/ui-react": "1.2.8",
    "@aws-sdk/lib-dynamodb": "3.26.0",
    "@hookform/resolvers": "2.7.1",
    "aws-amplify": "4.2.2",
    "axios": "0.21.4",
    "esbuild": "0.12.19",
    "jsonwebtoken": "8.5.1",
    "jwk-to-pem": "2.0.5",
    "loglevel": "1.7.1",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "7.12.2",
    "source-map-support": "0.5.19",
    "swr": "0.5.6",
    "web-vitals": "2.1.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@aws-cdk/aws-appsync": "1.120.0",
    "@aws-cdk/aws-cloudfront": "1.120.0",
    "@aws-cdk/aws-cloudfront-origins": "1.120.0",
    "@aws-cdk/aws-iam": "1.120.0",
    "@aws-cdk/aws-lambda": "1.120.0",
    "@aws-cdk/aws-lambda-nodejs": "1.120.0",
    "@aws-cdk/aws-s3": "1.120.0",
    "@aws-cdk/aws-s3-deployment": "1.120.0",
    "@aws-cdk/core": "1.120.0",
    "@aws-cdk/pipelines": "1.120.0",
    "@testing-library/jest-dom": "5.14.1",
    "@testing-library/react": "11.1.0",
    "@testing-library/user-event": "13.2.1",
    "@types/aws-lambda": "8.10.82",
    "@types/chai": "4.2.21",
    "@types/graphql": "14.5.0",
    "@types/mocha": "9.0.0",
    "@types/node": "13.13.52",
    "@types/react": "17.0.17",
    "@types/sinon": "10.0.2",
    "aws-cdk": "1.120.0",
    "aws-sdk": "2.967.0",
    "chai": "4.3.4",
    "copyfiles": "2.4.1",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "husky": "7.0.2",
    "mocha": "9.0.3",
    "mocha-chai-jest-snapshot": "1.1.3",
    "node-jq": "2.0.0",
    "npm-run-all": "4.1.5",
    "postcss": "8.3.6",
    "prettier": "2.3.2",
    "sinon": "11.1.2",
    "ts-node": "10.2.0",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.3.5"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build && next export",
    "start": "next start",
    "export": "next export",
    "lint:app": "next lint",
    "generate": "amplify codegen && copyfiles -f src/graphql/apiModel.ts backend/src/api/",
    "lint:prettier": "prettier --check 'backend/**/*.ts' 'src/**/*.ts'",
    "lint:tslint": "tslint --project tsconfig.json",
    "lint": "npm-run-all lint:*",
    "test:service": "TS_NODE_PROJECT='./tsconfig.backend.json' mocha -r ts-node/register \"backend/**/*.test.ts\"",
    "watch:test:service": "TS_NODE_PROJECT='./tsconfig.backend.json' mocha $NODE_DEBUG_OPTION --require ts-node/register --watch --watch-extensions ts --watch-files backend \"backend/**/*.test.ts\"",
    "test": "npm-run-all test:*",
    "deploy:backend": "cdk --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu\" deploy backend --require-approval never --outputs-file .cdk-backend-outputs.json",
    "deploy:frontend": "cdk --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu\" deploy frontend --require-approval never --outputs-file .cdk-frontend-outputs.json && ts-node --project=tsconfig.cdk.json deployment/bin/parse-cdk-outputs.ts",
    "deploy:app": "REACT_APP_API_URL= && npm run build && aws s3 sync out s3://hassu-app-$ENVIRONMENT",
    "synth": "cdk --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu\" synth",
    "destroy": "cdk --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu\" destroy '*' --require-approval never",
    "pipeline:bootstrap": "ACCOUNT_ARN=$(ts-node --project=tsconfig.cdk.json deployment/bin/get-account-arn.ts) && cdk bootstrap --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu-pipeline\" --cloudformation-execution-policies $ACCOUNT_ARN",
    "deploy:pipeline": "cdk --app \"ts-node --project=tsconfig.cdk.json ./deployment/bin/hassu-pipeline\" deploy hassu-pipeline --require-approval never"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": "14.17.0",
    "npm": "6.14.14"
  }
}
