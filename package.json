    "eslint": "7.32.0",
    "eslint-config-next": "11.1.4",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-mocha-no-only": "1.1.1",
    "graphql-schema-utilities": "1.1.5",
    "husky": "7.0.4",
    "identity-obj-proxy": "3.0.0",
    "jest": "27.5.1",
    "loader-utils": "1.4.2",
    "mocha": "9.2.2",
    "mocha-chai-jest-snapshot": "1.1.4",
