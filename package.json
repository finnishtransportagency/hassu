    "copyfiles": "2.4.1",
    "cross-env": "7.0.3",
    "cypress": "9.7.0",
    "cypress-file-upload": "5.0.8",
    "cypress-mochawesome-reporter": "3.2.3",
    "cypress-multi-reporters": "1.6.2",
    "deep-object-diff": "1.1.9",
